{
  "manifest_version": 3,
  "name": "2... 3... ש-גר!",
  "version": "1.0.0",
  "content_scripts": [
    {
      "matches": ["https://github.com/*"],
      "js": ["content.js"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["/audio.mp3"],
      "matches": ["https://github.com/*"]
    }
  ],
